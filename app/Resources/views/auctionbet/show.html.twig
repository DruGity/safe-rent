{% extends 'base.html.twig' %}

{% block body %}
    <h1>Auctionbet</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ auctionBet.id }}</td>
            </tr>
            <tr>
                <th>Advertid</th>
                <td>{{ auctionBet.advertId }}</td>
            </tr>
            <tr>
                <th>Fullsum</th>
                <td>{{ auctionBet.fullSum }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('auctionbet_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('auctionbet_edit', { 'id': auctionBet.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
