{% extends 'base.html.twig' %}

{% block body %}
    <h1>Commentphoto</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ commentPhoto.id }}</td>
            </tr>
            <tr>
                <th>Filename</th>
                <td>{{ commentPhoto.fileName }}</td>
            </tr>
            <tr>
                <th>Commentid</th>
                <td>{{ commentPhoto.commentId }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('commentphoto_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('commentphoto_edit', { 'id': commentPhoto.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
